import "std";

let x = +1.0f32 / 0.0f32;

func main() void {
    std::print_format_line(
        std::out(),
        "{}",
        (:[]std::formatter)[std::formatter::init[[f32]](&x)]);
    std::print_format_line(
        std::out(),
        "{}",
        (:[]std::formatter)[std::formatter::init[[f32]](&f32::INFINITY)]);
}
