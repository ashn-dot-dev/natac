import "sys";

func main() void {
    var x: usize = 1u;
    for x != 0u {
        for i in 1u:5u {
            if i < 3u {
                continue;
            }
            sys::dump[[usize]](i);
        }

        x = 0u;
        continue;
        sys::dump[[u32]](0xDEADBEEFu32);
    }
}
