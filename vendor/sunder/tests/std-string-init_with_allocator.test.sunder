import "std";
import "sys";

func main() void {
    var string = std::string::init_with_allocator(std::global_allocator());
    defer string.fini();

    sys::dump[[*byte]](string.start());
    sys::dump[[usize]](string.count());
    sys::dump[[usize]](string.capacity());
    sys::dump[[[]byte]](string.data());
}
