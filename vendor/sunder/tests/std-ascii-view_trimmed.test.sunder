import "std";

func main() void {
    std::print_line(std::out(), std::ascii::view_trimmed("foo"));
    std::print_line(std::out(), std::ascii::view_trimmed(" foo"));
    std::print_line(std::out(), std::ascii::view_trimmed("foo "));
    std::print_line(std::out(), std::ascii::view_trimmed(" foo "));
    std::print_line(std::out(), std::ascii::view_trimmed("\tfoo"));
    std::print_line(std::out(), std::ascii::view_trimmed("foo\t"));
    std::print_line(std::out(), std::ascii::view_trimmed("\tfoo\t"));
    std::print_line(std::out(), std::ascii::view_trimmed(""));
    std::print_line(std::out(), std::ascii::view_trimmed(" \t foo \t "));
}
