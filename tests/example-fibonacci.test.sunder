import "sys";

func fibonacci(n: usize) usize {
    if n == 0u {
        return 0u;
    }
    if n == 1u {
        return 1u;
    }
    return fibonacci(n-1u) + fibonacci(n-2u);
}

func main() void {
    sys::dump[[usize]](fibonacci(0u));
    sys::dump[[usize]](fibonacci(1u));
    sys::dump[[usize]](fibonacci(2u));
    sys::dump[[usize]](fibonacci(3u));
    sys::dump[[usize]](fibonacci(4u));
    sys::dump[[usize]](fibonacci(5u));
    sys::dump[[usize]](fibonacci(6u));
    sys::dump[[usize]](fibonacci(7u));
    sys::dump[[usize]](fibonacci(8u));
    sys::dump[[usize]](fibonacci(9u));
    sys::dump[[usize]](fibonacci(10u));
}
