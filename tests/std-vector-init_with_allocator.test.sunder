import "std";
import "sys";

func main() void {
    var vec = std::vector[[u32]]::init_with_allocator(std::global_allocator());
    defer vec.fini();

    sys::dump[[*u32]](vec.start());
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());
    sys::dump[[[]u32]](vec.data());
}
